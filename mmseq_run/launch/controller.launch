<?xml version="1.0"?>
<!-- Launches the ROS-based simulation, which uses the real MPC and MRT nodes talking to a PyBullet sim environment. -->

<launch>
  <arg name="config"/>
  <arg name="use_sim_time" default="false"/>

  <!-- use simulation time -->
  <param name="/use_sim_time" value="true" type="bool"/>


  <node pkg="mmseq_run" type="controller_ros.py" name="controller" output="screen" required="true" args="--config $(arg config)" />

</launch>